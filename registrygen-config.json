{
    "RegistryPrefix": "@hoogin-",
    "HomePage": "https://hoogin.be",
    "allowedExtensions": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx"
    ],
    "skipDirectories": [
        "node_modules",
        ".git",
        "dist",
        ".dist",
        "example",
        "examples"
    ],
    "knownRegistries": {
        "./src/components/ui": "@shadcn",
        "./src/@registry/auth": "@hoogin-auth",
        "./src/@registry/components": "@hoogin-components",
        "./src/@registry/form": "@hoogin-form",
        "./src/@registry/intlayer": "@hoogin-intlayer",
        "./src/@registry/layout": "@hoogin-layout"
    }
}