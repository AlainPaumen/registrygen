{
    "RegistryPrefix": "@hoogin-",
    "HomePage": "https://hoogin.be",
    "allowedExtensions": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx"
    ],
    "skipDirectories": [
        "node_modules",
        ".git",
        "dist",
        ".dist",
        "example",
        "examples"
    ],
    "knownRegistries": {
        "./src/components/ui": {
            "prefix": "@shadcn",
            "url": "http://localhost:4173/r/@shadcn"
        },
        "./src/@registry/auth": {
            "prefix": "@hoogin-auth",
            "url": "http://localhost:4173/r/@hoogin-auth"
        },
        "./src/@registry/components": {
            "prefix": "@hoogin-components",
            "url": "http://localhost:4173/r/@hoogin-components"
        },
        "./src/@registry/form": {
            "prefix": "@hoogin-form",
            "url": "http://localhost:4173/r/@hoogin-form"
        },
        "./src/@registry/intlayer": {
            "prefix": "@hoogin-intlayer",
            "url": "http://localhost:4173/r/@hoogin-intlayer"
        },
        "./src/@registry/layout": {
            "prefix": "@hoogin-layout",
            "url": "http://localhost:4173/r/@hoogin-layout"
        }
    }
}